{
  "name": "working-files",
  "main": "./lib/working-files",
  "version": "0.6.4",
  "description": "This package adds a 'working files' pane above Atom's tree-view, mimicking the functionality of Brackets.  Includes file icon support.",
  "keywords": [
    "tree-view",
    "files",
    "open"
  ],
  "repository": "https://github.com/davidashman/working-files",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-utils": "^0.9.2",
    "jquery": "^3.1.1",
    "lodash": "^2.4.2"
  },
  "devDependencies": {
    "pretty-format": "^4.2.1"
  },
  "consumedServices": {
    "file-icons.element-icons": {
      "versions": {
        "1.0.0": "consumeElementIcons"
      }
    }
  }
}
